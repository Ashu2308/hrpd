}
/*Updated CSS*/
body{
    color:#73879C;
    background:#F7F7F7;
}
.btn{
    padding:4px 8px;
}
.nav .fa{
    width:1.2em;
    font-size:1.2em ;
    text-align:left;
}
.dropdown>a{
    font-size:20px;
}
.navbar-brand{
    padding:0;
}
.navbar{
    border:none;
}
.table-responsive{
    padding:10px;
}
.panel-primary>.panel-heading{
    margin:0px 10px;
    background-color: #FFF;
    border-bottom: 2px solid #E6E9ED;
}
.panel-heading{
    padding:10px 0;
}
.panel-primary{
    border-color: #E6E9ED;
}
.table-responsive{
    font-size:13px;
}
.star{
    color: #F00;
    font-size: 12px;
    padding-left: 5px;
}
.timeEntry-control{
    float: right;
    margin-top: -27px;
    margin-right: 5px;
}

#print-area{
    clear: both;
}
aside h3{
    display: none;
}
#selected-values{
    display: none;
    color:#ff0;
}
#complaint-legends{
    margin-top: 130px;
}
#complaint-legends table tbody tr td{
    border: 1px solid #d4d4d4;
    padding: 5px;
    font-size: 13px;
}
#complaint-legends table tbody tr td:nth-child(2){
    text-align: right;
    font-weight: bold;
}
@media print{
    #complaint-legends table tbody tr td{
        font-size: 11px;
    }
    aside h3{
        border-bottom: 1px solid #E6E9ED;
        width: 56%;
        margin: 5px auto;
        display: block;
        text-align: center;
    }
    #print-area #selected-values{
        text-align: center;
        display: block;
    }
    #sel-form{
        display: none;
    }
    #selected-values{
        display: block;
        color: #ff0;
    }
    .panel-heading{
        display: none;
    }
    .print-head{
        border-top: 1px solid #E6E9ED; padding-top:20px;
    }
    #selected-values {
        color: #999 !important;
        text-shadow: 0 0 0 #999 !important;
    }
    #pie svg{
        width: 400px !important;
        padding-left: 0px;
        font-size: 10px;
        display: inline-block;
        float: left;
    }
    #complaint-legends{
        display: inline-block;
        float: left;
        font-size: 12px;
        
    }

    @media print and (-webkit-min-device-pixel-ratio:0) {
        #selected-values {
            color: #999 !important;
            -webkit-print-color-adjust: exact;
        }
    }
    @page{
        size:  auto;   /* auto is the initial value */
        margin: 0mm;  /* this affects the margin in the printer settings */
    }
    html{
        background-color: #FFFFFF; 
        margin: 0px;  /* this affects the margin on the html before sending to printer */
    }
    body{
        margin: 30px 20px 0px 20px; /* margin you want for the content */
    }
}

